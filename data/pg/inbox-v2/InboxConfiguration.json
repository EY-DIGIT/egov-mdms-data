{
  "tenantId": "pg",
  "moduleName": "INBOX",
  "InboxQueryConfiguration": [
    {
      "module": "RAINMAKER-PGR",
      "index": "pgr-services",
      "allowedSearchCriteria": [
        {
          "name": "area",
          "path": "Data.service.address.locality.code.keyword",
          "isMandatory": false,
          "operator": "ANY"
        },
        {
          "name": "assignee",
          "path": "Data.workflow.assignes.keyword",
          "isMandatory": false,
          "operator": "EQUAL"
        },
        {
          "name": "fromDate",
          "path": "Data.service.auditDetails.createdTime",
          "isMandatory": false,
          "operator": "GTE"
        },
        {
          "name": "toDate",
          "path": "Data.service.auditDetails.createdTime",
          "isMandatory": false,
          "operator": "LTE"
        },
        {
          "name": "complaintNumber",
          "path": "Data.service.serviceRequestId.keyword",
          "isMandatory": false,
          "operator": "EQUAL"
        },
        {
          "name": "serviceCode",
          "path": "Data.service.serviceCode.keyword",
          "isMandatory": false,
          "operator": "ANY"
        },
        {
          "name": "status",
          "path": "Data.currentProcessInstance.state.uuid.keyword",
          "isMandatory": false,
          "operator": "ANY"
        },
        {
          "name": "tenantId",
          "path": "Data.service.tenantId",
          "isMandatory": false,
          "operator": "EQUAL"
        }
      ],
      "sortBy": {
        "path": "Data.service.auditDetails.lastModifiedTime",
        "defaultOrder": "DESC"
      },
      "sourceFilterPathList": [
        "Data.service.serviceRequestId",
        "Data.service.address.locality",
        "Data.service.serviceCode",
        "Data.service.applicationStatus",
        "Data.workflow",
        "Data.service.user",
        "Data.service.auditDetails",
        "Data.currentProcessInstance",
        "Data.auditDetails"
      ]
    }
  ]
}
