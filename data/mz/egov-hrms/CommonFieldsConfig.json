              {
                "inline": true,
                "label": "HR_EMP_ID_LABEL",
                "isMandatory": true,
                "type": "text",
                "disable": false,
                "key": "SelectEmployeeId",
        
                "populators": {
                  "name": "SelectEmployeeId",
                  "error": "Required",
                  "validation": {
                    "pattern": "^[A-Za-z]+$",
        
                    "ValidationRequired": true
                  }
                }
              },
        
